#!/usr/bin/env sh

./gradlew -p .support/jellyfin-gradle publishAllPublicationsToProjectLocalRepository
./gradlew -p .support/jellyfin-lint publishAllPublicationsToProjectLocalRepository

find .support/.m2 -type f \( -name "*.md5" -o -name "*.sha1" -o -name "*.sha256" -o -name "*.sha512" \) -exec rm -f {} \;
